{
  "manifest_version": 3,
  "name": "Good first issue",
  "version": "1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "lookup.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "storage.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "options_page": "options.html"
}

